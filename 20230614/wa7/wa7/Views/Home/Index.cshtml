@model List<Northwind.Model.Product>

@{
    ViewData["Title"] = "Home Page";
}

@if (Model != null && Model.Count() > 0)
{
    <table class="table">
        <tbody>
            @foreach (var p in Model)
            {
                <tr><td>@p.Name</td><td>@p.Price</td></tr>
            }
        </tbody>
    </table>
}
